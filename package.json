{
  "name": "planktos",
  "version": "0.3.0",
  "description": "Serving websites over bittorrent",
  "main": "index.js",
  "bin": "./bin/setup.js",
  "dependencies": {
    "chunk-store-read-stream": "^1.0.0",
    "create-torrent": "^3.26.1",
    "idb-kv-store": "^4.1.0",
    "indexeddb-chunk-store": "^2.2.0",
    "inherits": "^2.0.3",
    "minimist": "^1.2.0",
    "parse-torrent-file": "^4.0.1",
    "readable-stream": "^2.2.2",
    "run-parallel-limit": "^1.0.3",
    "stream-to-blob": "^1.0.0",
    "tab-elect": "^0.1.0",
    "webtorrent": "^0.98.0"
  },
  "devDependencies": {
    "browserify": "^14.0.0",
    "exorcist": "^0.4.0",
    "express": "^4.14.0",
    "karma": "^1.4.0",
    "karma-browserify": "^5.1.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sauce-launcher": "^1.1.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "standard": "^8.6.0",
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony-v2.7.5",
    "watchify": "^3.9.0"
  },
  "scripts": {
    "standard": "standard",
    "bundle": "browserify . -s TorrentWorker | uglifyjs -m -c warnings=false > torrentworker.min.js",
    "test": "standard && karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuset/planktos.git"
  },
  "keywords": [
    "bittorrent",
    "webtorrent",
    "p2p",
    "static",
    "seed"
  ],
  "standard": {
    "globals": [
      "URL",
      "Response",
      "BroadcastChannel",
      "Blob"
    ],
    "ignore": [
      "build",
      "test/www"
    ]
  },
  "author": "xuset",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xuset/planktos/issues"
  },
  "homepage": "https://xuset.github.io/planktos/"
}
